# API Keys and Credentials
OPENAI_API_KEY=your_openai_api_key_heres
TRELLO_API_KEY=your_trello_api_key_heres
TRELLO_API_TOKEN=your_trello_api_token_heres
TRELLO_BOARD_ID=your_trello_board_id_herez
CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret
SLACK_BOT_TOKEN=your_slack_bot_token_here
SLACK_DEFAULT_CHANNEL=your_slack_channelID_here
AGENTVERSE_TOKEN=your_agentverse_token_here
AGENTVERSE_URL=https://agentverse.ai
AGENTVERSE_SSL_VERIFY=true
# Application Settings
APP_ENV=development  # Options: development, production, testing
LOG_LEVEL=INFO  # Options: DEBUG, INFO, WARNING, ERROR, CRITICAL

# Output Settings
OUTPUT_DIR=./reports
REPORT_FORMAT=markdown  # Options: markdown, html, pdf

# Notification Settings
ENABLE_NOTIFICATIONS=false
NOTIFICATION_EMAIL=yourname@example.com
# Slack integration settings
SLACK_BOT_TOKEN=xoxb-your-bot-token-here
SLACK_DEFAULT_CHANNEL=sprint-reports

# Model Settings
DEFAULT_MODEL=gpt-4  # Adjust based on your preference and availability
MODEL_TEMPERATURE=0.3
MAX_TOKENS=4096

# Application Customization
TEAM_NAME=Your Team Name
COMPANY_NAME=Your Company

# Performance Settings
CACHE_ENABLED=true
CACHE_TTL=3600  # seconds
MAX_RETRIES=3
RETRY_DELAY=2  # seconds

# DO NOT commit the actual .env file to version control!